                            }
                        }
                    }
                }
            }
        },
        "/uritoken/uritokenid/{uritokenid}": {
            "get": {
                "summary": "Get details for a single UriToken.",
                "description": "Returns a single UriToken and its details.",
                "tags": [
                    "UriToken Methods"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "UriTokenID",
                        "required": true,
                        "description": "The UriTokenID of the UriToken you want to get the details for.",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "- `uritoken` = single UriToken object for the given UriTokenID",
                        "headers": {
                            "x-ratelimit-limit": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Maximum number of requests per minute."
                            },
                            "x-ratelimit-remaining": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of remaining requests per minute."
                            },
                            "x-ratelimit-reset": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of seconds until the current rate limit window resets."
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UriTokenDetailResponse"
                                }
                            }
                        }
                    }
                }
            }
        },"/uritoken/uri": {
            "post": {
                "summary": "Get all UriTokens with a specific URI",
                "description": "Returns an array of UriTokens with a specific URI",
                "tags": [
                    "UriToken Methods"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/limit"
                    },
                    {
                        "$ref": "#/components/parameters/skip"
                    }
                ],
                "requestBody": {
                    "description": "The URI to search UriTokens for.",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "required": ["uri"],
                                "properties": {
                                    "uri": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "- `uritokens` = Array of all UriTokens with the specified URI",
                        "headers": {
                            "x-ratelimit-limit": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Maximum number of requests per minute."
                            },
                            "x-ratelimit-remaining": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of remaining requests per minute."
                            },
                            "x-ratelimit-reset": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of seconds until the current rate limit window resets."
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/URIUriTokenResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
