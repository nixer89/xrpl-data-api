    "paths": {
        "/uritoken/all/issuers": {
            "get": {
                "summary": "Get all current Issuers of UriTokens",
                "description": "Returns an array of XRPL accounts which have currently issued UriTokens on the XRP Ledger.",
                "tags": [
                    "UriToken Methods"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/limit"
                    },
                    {
                        "$ref": "#/components/parameters/skip"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "- `issuers` = Array of all currently known UriTokenoken Issuers",
                        "headers": {
                            "x-ratelimit-limit": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Maximum number of requests per minute."
                            },
                            "x-ratelimit-remaining": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of remaining requests per minute."
                            },
                            "x-ratelimit-reset": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of seconds until the current rate limit window resets."
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AllIssuerResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/uritoken/issuer/{issuer}": {
            "get": {
                "summary": "Get all UriTokens from a specific Issuer",
                "description": "Returns an array of UriTokens issued by the given Account",
                "tags": [
                    "UriToken Methods"
                ],
                "parameters": [
                    {
                        "in": "path",
                        "name": "issuer",
                        "required": true,
                        "description": "The issuer account you want to get the UriTokens for.",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "$ref": "#/components/parameters/limit"
                    },
                    {
                        "$ref": "#/components/parameters/skip"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "- `uritokens` = Array of all UriTokens of the given Issuer",
                        "headers": {
                            "x-ratelimit-limit": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Maximum number of requests per minute."
                            },
                            "x-ratelimit-remaining": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of remaining requests per minute."
                            },
                            "x-ratelimit-reset": {
                                "schema": {
                                    "type": "integer"
                                },
                                "description": "Number of seconds until the current rate limit window resets."
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/IssuerUriTokenResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
